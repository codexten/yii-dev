{
  "name": "codexten/yii-dev",
  "type": "yii-extension",
  "description": "YiiSoft vendor configuration for HiDev",
  "keywords": [
    "hidev"
  ],
  "minimum-stability": "dev",
  "require": {
    "hiqdev/composer-config-plugin": "*@dev",
    "hiqdev/hidev": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\dev\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "sconfig-plugin": {
      "params": "src/config/params.php",
      "core": "src/config/core.php",
      "hidev": [
        "$core",
        "$nginx",
        "src/config/hidev.php"
      ]
    }
  }
}
